 <h1>Listing jobs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>JobType</th>
    <th>Salary</th>
    <th>Location</th>
    <th>Description</th>
    <th colspan="3"></th>
  </tr>
 
  <% @jobs.each do |job| %>
    <tr>
      <td><%= job.title %></td>
      <td><%= job.jobtype %></td>
      <td><%= job.salary %></td>
      <td><%= job.location %></td>
      <td><%= job.description %></td>
      <td><%= link_to 'Show', applied_job_path(job) %></td>
      
    </tr>
  <% end %>
</table>


<%= button_to 'logout', destroy_user_session_path,method: :delete %>


<% flash.each do |key, value| %>
  <div class="alert alert-<%= key %>"><%= value %></div>
<% end %>