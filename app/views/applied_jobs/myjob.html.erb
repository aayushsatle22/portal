 <center><h1><b>MY JOBS</b></h1></center>

<table class="table table-striped">
  <table class="table align-middle">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Title</th>
      <th scope="col">JobType</th>
      <th scope="col">Salary</th>
      <th scope="col">Location</th>
      <th scope="col">Description</th>
    </tr>
  </thead>
 
  <% @jobs.each do |record| %>
    <tr>
      <td><%= record.id %></td>
      <td><%= record.title %></td>
      <td><%= record.jobtype %></td>
      <td><%= record.salary %></td>
      <td><%= record.location %></td>
      <td><%= record.description %></td>
      
    </tr>
  <% end %>
</table>

<center><%= link_to 'back',  applied_jobs_path,class: "btn btn-secondary" %></center>



































