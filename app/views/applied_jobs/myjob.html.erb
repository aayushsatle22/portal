 <h1>MY JOBS</h1>
 

<table>
  <tr>
    <th>Title</th>
    <th>JobType</th>
    <th>Salary</th>
    <th>Location</th>
    <th>Description</th>
    <th colspan="3"></th>
  </tr>
 
  <% @jobs.each do |record| %>
    <tr>
      <td><%= record.title %></td>
      <td><%= record.jobtype %></td>
      <td><%= record.salary %></td>
      <td><%= record.location %></td>
      <td><%= record.description %></td>
      <td><%= link_to 'back',  applied_jobs_path %></td>
      
    </tr>
  <% end %>
</table>





































