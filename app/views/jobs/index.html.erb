<h1>ADMIN PORTAL</h1>

 <h1>Listing jobs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>JobType</th>
    <th>Salary</th>
    <th>Location</th>
    <th>Description</th>
    <th colspan="3"></th>
  </tr>
 
  <% @jobs.each do |job| %>
    <tr>
      <td><%= job.title %></td>
      <td><%= job.jobtype %></td>
      <td><%= job.salary %></td>
      <td><%= job.location %></td>
      <td><%= job.description %></td>
      <td><%= link_to 'Show', job_path(job) %></td>
      <td><%= link_to 'Edit', edit_job_path(job) %></td>
       <td><%= link_to 'Destroy', job_path(job),
              method: :delete,
              data: { "turbo-method": :delete, confirm: 'Are you sure?'  }
               %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'New job', new_job_path %>
<%= link_to 'All Users', all_user_jobs_path %>
<%= link_to 'Applicant', applicant_jobs_path %>
<%= button_to 'logout', destroy_user_session_path,method: :delete %>
