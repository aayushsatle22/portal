<h1>Registered User</h1>


<table>
  <tr>
    <th>Name</th>
    <th>Surname</th>
    <th>Email</th>
    <th>Role</th>
    <th colspan="3"></th>
  </tr>
 
  <% @all_user.each do |record| %>
    <tr>
      <td><%= record.name %></td>
      <td><%= record.surname %></td>
      <td><%= record.email %></td>
      <td><%= record.role %></td>
      
      
    </tr>
  <% end %>
</table>

